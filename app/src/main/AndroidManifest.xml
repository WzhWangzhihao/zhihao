<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.asia_eagle.money.erqing">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <!-- 加入在SDCard中创建与删除文件的权限 -->
    <uses-permission android:name="android.permission.MOUNT_UNMOUNT_FILESYSTEMS" />
    <!-- 允许应用读取低级别的系统日志文件 -->
    <uses-permission android:name="android.permission.READ_LOGS" />
    <uses-permission android:name="android.permission.WRITE_SETTINGS" />
    <!-- 允许应用拨打电话 -->
    <uses-permission android:name="android.permission.CALL_PHONE" />
    <!-- 相机 -->
    <uses-permission android:name="android.permission.CAMERA" />
    <!-- 允许读取用户数据的权限 -->
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.GET_TASKS" />
    <uses-permission android:name="android.permission.CHANGE_WIFI_STATE" />
    <uses-permission android:name="android.permission.MANAGE_ACCOUNTS" />
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />

    <uses-permission android:name="android.permission.FLASHLIGHT" />

    <uses-feature android:name="android.hardware.camera" />

    <uses-feature android:name="android.hardware.autofocus" />
    <!-- 打印机权限 -->
    <uses-permission android:name="android.permission.CLOUDPOS_PRINTER" />
    <application
        android:name=".activity.App"
        android:allowBackup="true"
        android:icon="@mipmap/icon"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.NoActionBar">
        <activity
            android:name=".activity.LoginAct"
            android:screenOrientation="portrait"></activity>
        <activity
            android:name=".activity.StartPageAct"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activity.WxPayScanningAct"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.OrderdetailsAct"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.AliPayScanningAct"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.QqPayScanningAct"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.ReceivablesAct"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.HomeAct"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.OkReceivablesAct"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.OverPayAct"
            android:screenOrientation="portrait" />
    </application>

</manifest>